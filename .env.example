BETTER_AUTH_SECRET=test
BETTER_AUTH_URL=http://localhost:3000

DATABASE_URL="postgres://postgres:postgres@localhost:51214/template1?sslmode=disable&connection_limit=1&connect_timeout=0&max_idle_connection_lifetime=0&pool_timeout=0&single_use_connections=true&socket_timeout=0"
SHADOW_DATABASE_URL="postgres://postgres:postgres@localhost:51215/template1?sslmode=disable&connection_limit=1&connect_timeout=0&max_idle_connection_lifetime=0&pool_timeout=0&single_use_connections=true&socket_timeout=0"

# PWA Push Notifications (VAPID Keys)
# Generate keys using: npx web-push generate-vapid-keys
# Or install globally: npm install -g web-push && web-push generate-vapid-keys
NEXT_PUBLIC_VAPID_PUBLIC_KEY=your_public_key_here
VAPID_PRIVATE_KEY=your_private_key_here
VAPID_SUBJECT=mailto:your-email@example.com

# Vercel AI SDK – OpenAI-compatible endpoint (any provider: OpenAI, Azure, OpenRouter, local LLM, etc.)
# Leave baseURL empty to use OpenAI's default; set to your provider's base URL otherwise.
AI_OPENAI_COMPATIBLE_BASE_URL=https://api.openai.com/v1
AI_OPENAI_COMPATIBLE_API_KEY=your_api_key_here

# Manus API – for AI destination suggestions (lib/manus.service.ts).
# If set, used by the Manus service; otherwise AI_OPENAI_COMPATIBLE_API_KEY is used when pointing at Manus.
MANUS_API_KEY=your_manus_api_key_here
